<div class = 'main-header'>
  <h1 class = 'main-header no-search'>Outfits - <%= @contact.name %><span class = 'btn pull-right add-button'><%= link_to '+', new_outfit_path %></span></h1>
</div>
<% @contact.outfits.each do |outfit| %>
	<div class = 'outfit-photo'>
	  <%= image_tag outfit.filename %>
	  <p><%= outfit.date_time.getlocal %></p>
	</div>
<% end %>